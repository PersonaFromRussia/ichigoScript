local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "IchigoScript", HidePremium = false, SaveConfig = true, ConfigFolder = ""})

local Tab = Window:MakeTab({
	Name = "Player",
	Icon = "",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Jump",
	Default = "default box input",
	TextDisappear = true,
	Callback = function(Value)
		print(Value)
	end	  
})
